{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion":"latest",
        "sourceType":"module"
    },
    "rules": {
        "indent":["warn",4],
        "semi":["error","always"],
        "quotes":["warn","double"],
        "linebreak-style":["warn","windows"],
        "no-console":"off"
    }
}
